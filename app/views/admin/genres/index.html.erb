<h2>ジャンル一覧・追加</h2>


  <%= form_with model: @genre,url:admin_genres_path do |f| %>

    <h4>ジャンル名</h4>
      <%= f.text_field :name %>

    <%= f.submit '新規登録' %>
  <% end %>

  <% @genres.each do |genre|%>
  <%= genre.name %><br>
  <%= link_to "編集する", edit_admin_genre_path(genre.id) %>
  <% end %>



